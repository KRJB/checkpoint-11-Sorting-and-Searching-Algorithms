ALGORITHM Insertion sort
VAR
    arr : ARRAY_OF INTEGER[n];
    i, temp,j: INTEGER;
BEGIN
    write ('insert length of array');
    read (n);
    FOR i FROM 1 TO n-1  DO
        temp := arr[i];
        j := i-1;
        WHILE (j > 0 and arr[j] > temp ) DO
            arr[j+1]:= arr[j]
            j:= j-1
            arr[j+1]:= temp
        END_WHILE
    END_FOR
END
